#include "main.h"

/**
 * _strcmp - appends the src string to the dest string,
 * @s1:the first string
 * @s2:the second string
 *
 * Return: return dest.
 */

int _strcmp(char *s1, char *s2)
{
int i = 0, cmp = 0;

while (cmp == 0)
{
if ((*(s1 + i) == '\0') && (*(s2 + i) == '\0'))
break;
cmp = *(s1 + i) - *(s2 + i);
i++;
}
return (cmp);
}
